{
    "compilerOptions": {
        "target": "ES5",
        "module": "AMD",
        "rootDir": "src",
        "outDir": "dist",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": false,
        "forceConsistentCasingInFileNames": false,
        "importHelpers": false,
        "strict": true,
        "lib": [
            "ES2015",
            "DOM"
        ]
    }
}